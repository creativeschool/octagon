{
  "name": "octagon",
  "version": "0.1.5",
  "private": true,
  "description": "Management App for Hexagon",
  "author": "ZhangZisu <admin@zhangzisu.cn>",
  "scripts": {
    "serve": "vue-cli-service electron:serve",
    "build": "vue-cli-service electron:build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "mongodb": "^3.3.0-beta2"
  },
  "devDependencies": {
    "@mdi/font": "^3.8.95",
    "@types/mongodb": "^3.1.31",
    "@types/node": "^14.0.23",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "ajv": "^6.10.2",
    "babel-eslint": "^10.0.1",
    "crypto-random-string": "^3.0.1",
    "deepmerge": "^4.0.0",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "electron-icon-builder": "^1.0.1",
    "electron-updater": "^4.1.2",
    "escape-string-regexp": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.1",
    "git-revision": "^0.0.2",
    "marked": "^0.7.0",
    "sass": "^1.22.9",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-cli-plugin-electron-builder": "^1.3.6",
    "vue-router": "^3.0.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify": "^2.0.4",
    "vuetify-loader": "^1.3.0",
    "xlsx": "^0.14.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "Apache-2.0",
  "repository": "https://github.com/creativeschool/octagon"
}
